{
    "name": "oro/flexible-entity-bundle",
    "type": "symfony-bundle",
    "description": "BAP Flexible Entity Bundle",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/FlexibleEntityBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.1.6,<=2.1.7"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.2,<2.4-dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\FlexibleEntityBundle": "" }
    },
    "target-dir": "Oro/Bundle/FlexibleEntityBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
