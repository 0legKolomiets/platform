{
    "name": "oro/user-bundle",
    "type": "symfony-bundle",
    "description": "BAP User Bundle",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/UserBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "friendsofsymfony/user-bundle": "*",
        "friendsofsymfony/rest-bundle": "*",
        "oro/FlexibleEntityBundle": "dev-master",
